  // FFT_CPX_T<DT> scratch_in;
  FFT_CPX_T<DT> Fin[radix];
  // FFT_CPX_T<DT> Fout[radix];

  // __memcpy(scratch_tw_r, sram_tw, butterfly_num * (3 - 1) * 2, SRAM2NRAM);

