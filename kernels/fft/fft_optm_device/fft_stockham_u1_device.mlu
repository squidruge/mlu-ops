    __memcpy(&nram_out_i[Fout_stride], &out.i[sec_count * butterfly_num],
             butterfly_num * sizeof(DT), NRAM2NRAM, butterfly_num * sizeof(DT),
             align_N * sizeof(DT), radix - 1);

    // Fin_stride += butterfly_num;
    Fout_stride += radix * butterfly_num;
  }
}

